{
  "private": true,
  "scripts": {
    "start": "umi dev",
    "build": "umi build",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@ant-design/icons": "^4.3.0",
    "@umijs/preset-react": "1.7.9",
    "@umijs/test": "3.3.4",
    "ace-builds": "^1.4.12",
    "dexie": "3.0.3",
    "lint-staged": "10.5.3",
    "prettier": "2.2.1",
    "react": "17.0.1",
    "react-ace": "^9.2.1",
    "react-dom": "17.0.1",
    "react-markdown": "4.3.1",
    "serve": "^11.3.2",
    "umi": "^3.3.4",
    "yorkie": "2.0.0"
  },
  "devDependencies": {
    "compression-webpack-plugin": "^8.0.0",
    "umi-webpack-bundle-analyzer": "^3.6.0"
  }
}
