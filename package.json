{
  "private": true,
  "scripts": {
    "start": "umi dev",
    "build": "umi build",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@ant-design/icons": "4.1.0",
    "@umijs/preset-react": "1.4.22",
    "@umijs/test": "3.1.4",
    "ace-builds": "1.4.11",
    "dexie": "3.0.1",
    "lint-staged": "10.2.2",
    "prettier": "2.0.5",
    "react": "16.13.1",
    "react-ace": "8.1.0",
    "react-dom": "16.13.1",
    "react-markdown": "4.3.1",
    "serve": "^11.3.0",
    "umi": "3.2.7",
    "yorkie": "2.0.0"
  }
}
