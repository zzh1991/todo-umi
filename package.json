{
  "private": true,
  "scripts": {
    "start": "umi dev",
    "build": "umi build",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@ant-design/icons": "^4.0.2",
    "@umijs/preset-react": "^1.4.3",
    "@umijs/test": "^3.0.8",
    "ace-builds": "^1.4.8",
    "dexie": "^2.0.4",
    "lint-staged": "^10.0.7",
    "prettier": "^1.19.1",
    "react": "^16.12.0",
    "react-ace": "^8.0.0",
    "react-dom": "^16.12.0",
    "react-markdown": "^4.3.1",
    "umi": "^3.0.8",
    "umi-plugin-antd-icon-config": "^2.0.0",
    "yorkie": "^2.0.0"
  }
}
